
          <!-- chat section -->
          <div class="chat-messages_2">
            <div>
              <div class="messages-head d-flex justify-content-between">
                <div class="d-flex">
                  <div class="chat-block_flex">
                    <div class="chat-block_flex_img_container">
                      <img
                        src="/assets/images/chat-img.png"
                        class="img-fluid"
                        alt="chat-logo"
                      />
                    </div>
                    <div class="pl-2">
                      <strong><%= interestData.interestName %></strong>
                      <figcaption class="chat-intro">
                        <span><%= interestData.interestMembers.length %> members , </span><span> <%= interestData.onlineMembers %> online</span>
                      </figcaption>
                      
                    </div>
                  </div>
                </div>
                <div class="grey-icons d-flex">
                  <div class="search-bar">
                    <!-- <img
                      class="search-icon"
                      src="/assets/svgs/Icons-search-gray.svg"
                      alt=""
                    /> -->
                    <div class="chat-search-icon">
                      <span
                        class="iconify"
                        data-icon="carbon:search"
                        data-inline="false"
                      ></span>
                    </div>

                    <form class="chat-search inline-form" action="">
                      <input
                        class="profile-search chat-search"
                        type="text"
                        placeholder="Search"
                      />
                      <img
                        style="align-self: baseline"
                        src="/assets/svgs/tick.svg"
                        alt=""
                      />
                    </form>
                  </div>

                  <!-- mutumina -->
                  <div class="dropdown" style="margin-top: -11px">
                    <div
                      type="button"
                      id="dropdownMenuButton"
                      data-toggle="dropdown"
                      aria-haspopup="true"
                      aria-expanded="false"
                    >
                      <span
                        class="iconify"
                        data-icon="ph:dots-three-vertical-bold"
                        data-inline="false"
                      ></span>
                    </div>
                    <div
                      class="dropdown-menu"
                      aria-labelledby="dropdownMenuButton"
                    >
                      <a class="dropdown-item" href="#">
                        <span
                          class="iconify"
                          data-icon="carbon:arrows-vertical"
                          data-inline="false"
                        ></span>
                        Sort By (A/Z,Z/A)
                      </a>
                      <a class="dropdown-item" href="/feedback">
                        <span
                          class="iconify"
                          data-icon="codicon:feedback"
                          data-inline="false"
                        ></span>
                        Feedback
                      </a>
                      <a class="dropdown-item" href="/profile"
                        ><span
                          class="iconify"
                          data-icon="bytesize:user"
                          data-inline="false"
                        ></span
                        >My Profile</a
                      >
                    </div>
                  </div>
                </div>
              </div>
              <hr />




              <!-- message part -->

              <div class="message-box">
                <div class="msnger-chat">
                <div class="messagebody" id="messagebody">                 

                </div>
              </div>
            </div>




              <!-- input part -->

              <div class="type-box">
                <div class="type-container">
                  <div class="type-group">
                    <form  id="image-form">
                      <label
                        class="attach-img"
                        id="image-upload-label"
                        for="file-upload"
                      >
                        <input
                          accept="image/*"
                          name="picture"
                          type="file"
                          id="file-upload"
                         />

                        <span
                          class="iconify"
                          data-icon="subway:pin"
                          data-inline="false"
                        ></span>
                      </label>
                    </form>
                    <form>
                     <div class="pr-4"></div>
                     <div class="d-flex fake-text-field">
                      <div id="emoji">
                        <span
                          class="iconify"
                          data-icon="ph:smiley-fill"
                          data-inline="false"
                        ></span>
                      </div>
                      <input
                        type="text"
                        placeholder="Type your messages"
                        class="text-field"
                        id="chatinput"
                      />
                      <div class="sendMessage">
                        <!-- add onclick function to send message -->
                         <button style=" border: 0px white; background-color:transparent;"> 
                          <span
                          class="iconify"
                          data-icon="fluent:send-24-filled"
                          data-inline="false"
                        
                        ></span>
                        </button>
                      </div>
                      </form>
                     </div>
                    <!-- </form> -->
                  </div>
                </div>
              </div>
            </div>
            
          </div> 

          <%- include ('memberssection.ejs')%>
          </div>
